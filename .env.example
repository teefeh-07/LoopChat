# ChainChat Environment Variables
# Copy this file to .env and fill in your values

# Reown (WalletConnect) Project ID
# Get yours at: https://cloud.reown.com/
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Network Configuration (testnet or mainnet)
VITE_NETWORK=mainnet

# Contract Addresses (Mainnet - Deployed)
VITE_VAULT_CONTRACT=SPD5ETF2HZ921C8RJG2MHPAN7SSP9AYEYD5GSP84.strategy-vaultx
VITE_ENGINE_CONTRACT=SPD5ETF2HZ921C8RJG2MHPAN7SSP9AYEYD5GSP84.strategy-enginex

# Hiro Platform API & Chainhooks Configuration
# Get your API key from: https://platform.hiro.so
HIRO_API_KEY=your_hiro_api_key_here

# Chainhook Webhook Secret (generate a secure random string)
CHAINHOOK_WEBHOOK_SECRET=generate_a_secure_random_secret_here

# Webhook Server Port
PORT=3001
