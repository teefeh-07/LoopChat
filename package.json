{
  "name": "loop-chat",
  "version": "1.0.0",
  "description": "Run unit tests on this project.",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:unit": "vitest run tests/access-control.test.ts",
    "test:security": "vitest run tests/security.test.ts",
    "test:integration": "vitest run tests/integration.test.ts",
    "test:property": "vitest run tests/property-based.test.ts",
    "test:fuzzing": "vitest run tests/fuzzing.test.ts",
    "test:gas": "vitest run tests/gas-optimization.test.ts",
    "test:report": "vitest run -- --coverage --costs",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "prepare": "husky install",
    "dev": "vite --config vite.config.frontend.js",
    "build": "vite build --config vite.config.frontend.js",
    "preview": "vite preview --config vite.config.frontend.js",
    "start": "npm run dev",
    "server": "node server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "chainhook:start": "chainhook service start --config-path=Chainhook.toml",
    "chainhook:register-all": "npm run chainhook:register:vault && npm run chainhook:register:staking && npm run chainhook:register:rewards && npm run chainhook:register:emergency && npm run chainhook:register:liquidation",
    "chainhook:register:vault": "chainhook predicates scan chainhooks/vault-events.json --config-path=Chainhook.toml",
    "chainhook:register:staking": "chainhook predicates scan chainhooks/staking-events.json --config-path=Chainhook.toml",
    "chainhook:register:rewards": "chainhook predicates scan chainhooks/rewards-events.json --config-path=Chainhook.toml",
    "chainhook:register:emergency": "chainhook predicates scan chainhooks/emergency-events.json --config-path=Chainhook.toml",
    "chainhook:register:liquidation": "chainhook predicates scan chainhooks/liquidation-events.json --config-path=Chainhook.toml",
    "chainhook:list": "chainhook predicates list --config-path=Chainhook.toml",
    "mainnet:test": "tsx scripts/mainnet-test.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hirosystems/chainhooks-client": "^1.0.0",
    "@hirosystems/clarinet-sdk": "^2.14.0",
    "@reown/appkit": "^1.8.15",
    "@reown/appkit-core": "^1.8.15",
    "@stacks/common": "^6.12.0",
    "@stacks/connect": "^8.2.2",
    "@stacks/network": "^7.3.1",
    "@stacks/transactions": "^6.12.0",
    "@stacks/wallet-sdk": "^7.2.0",
    "@walletconnect/encoding": "^1.0.2",
    "@walletconnect/qrcode-modal": "^1.8.0",
    "@walletconnect/sign-client": "^2.23.1",
    "@walletconnect/types": "^2.23.1",
    "@walletconnect/utils": "^2.23.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0",
    "typescript": "^5.6.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.0",
    "vitest-environment-clarinet": "^2.3.0"
  },
  "main": "vitest.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gboigwe/ChainChat.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/gboigwe/ChainChat/issues"
  },
  "homepage": "https://github.com/gboigwe/ChainChat#readme",
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "terser": "^5.44.1",
    "tsx": "^4.21.0",
    "util": "^0.12.5"
  }
}